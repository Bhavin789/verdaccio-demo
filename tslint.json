{
    "extends": [
      "tslint:latest", 
      "tslint-react", 
      "tslint-plugin-prettier",
      "tslint-config-prettier"
    ],
    "rules": {
      "semicolon": "single",
      "prettier": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template",
      "jsx-double"
    ]
    }
}